<%= form_with model: @book, local: true do |form| %>
    <p>
        <%= form.label :title %>
        <%= form.text_field :title %>
    </p>
    <p>
        <%= form.label :page_numbers %>
        <%= form.text_field :page_numbers %>
    </p>
    <p>
        <%= form.label :dewey_decimal %>
        <%= form.text_field :dewey_decimal %>
    </p>
    <%= form.submit %>
<% end %>